@model List.Models.Item
<form method="POST" asp-action="Delete" class="border border-info p-3" style="border-radius: 5px;">
    <label asp-for="ItemName"></label>
    <input disabled type="text" asp-for="ItemName" class="form-control d-block m-2" style="cursor: not-allowed;" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-lg">
    <label asp-for="Brrower"></label>
    <input disabled type="text" asp-for="Brrower" class="form-control d-block m-2" style="cursor: not-allowed;"  aria-label="Sizing example input" aria-describedby="inputGroup-sizing-lg">
    <label asp-for="Lender"></label>
    <input disabled type="text" asp-for="Lender" class="form-control d-block m-2" style="cursor: not-allowed;"  aria-label="Sizing example input" aria-describedby="inputGroup-sizing-lg" >
    <input type = "submit" class="m-2 btn btn-danger w-100" value="Delete">
    <a asp-action="index" class = "m-2 btn btn-success w-100">Back</a>
</form>